let array1 = [5,12,8,130,44];

let resA1 = array1.find( element => element > 10 );
let resA2 = array1.includes(8);
let resA3 = array1.findIndex( el => el == 44);
let resA4 = array1.some( element => element % 2 === 0);
let resA5 = array1.some( elemento => elemento % 2 >= 1000 );

console.log("primer elemento encontrado" + " " + resA1);
console.log("segundo cp encontrado" + " " + resA2);
console.log("indice del elemento encontrado" + " " + resA3);
console.log(resA4);
console.log(resA5);

let map1 = array1.map( el => el * 2);
console.log("elementos multiplicados" + " " + map1);

