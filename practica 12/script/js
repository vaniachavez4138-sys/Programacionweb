    "username" : "cplio",
    "score" : 67,
    "hours" : 167
};

console.log(objetoUsuario);

objetoUsuario.profesional = false;
console.log(objetoUsuario);
console.log(objetoUsuario.username);

class Usuario {

    constructor(username,score,hours,profesional){
        this.username = username;
        this.score = score;
        this.hours = hours;
        this.profesional = profesional;
    }

    getName(){
        return this.username;
    }
    setName(){
        this.username = nombres;
    }
}

let objetoUsuario2 = new Usuario(
    "Ricardo",
    67,
    280,
    true
);

console.log(objetoUsuario2);
console.log(objetoUsuario2.username);
objetoUsuario2.setName("Miguel");

objetoUsuario2.username = "Miguel";
console.log(objetoUsuario2.username + " " + objetoUsuario2.username + " " + "agarra mi chile y juega con el");

let inventario = [
    { name: "Manzanas", cantidad: 10 },
    { name: "Peras", cantidad: 50 },
    { name: "Uvas", cantidad: 40 },
    { name: "Naranjas", cantidad: 67 },
];

let resultadoInventario = inventario.find(
    el => el.name == "Manzanas"
);
console.log(resultadoInventario);

let resultadoInventario2 = inventario.find(
    ({name}) => name == "Peras"
);
console.log(resultadoInventario2);

let resultadoInventario3 = inventario.find(
    ({cantidad}) => cantidad > 5
);
console.log(resultadoInventario3);
